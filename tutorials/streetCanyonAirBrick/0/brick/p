FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 100000;
    }
    bottom
    {
        type            calculated;
        value           uniform 100000;
    }
    interior
    {
        type            calculated;
        value           uniform 100000;
    }
    brick_to_air      
    {
        type            calculated;
        value           uniform 100000;
    }
    sides
    {
        type            empty;
    }
}

